{
  "name": "apollonius",
  "version": "1.2.1",
  "description": "Solves Apollonius' problem by finding a fourth circle tangent to three given circles",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "standard",
    "test:unit": "node test/index.js",
    "test:types": "echo \"Expect error:\" && ! tsc test/types.ts",
    "test:cjs": "node test/commonjs.cjs",
    "test": "npm run lint && npm run test:types && npm run build && npm run test:cjs && npm run test:unit",
    "release": "npm test && npm publish"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/axelpale/apollonius.git"
  },
  "keywords": [
    "2d",
    "math",
    "tangent",
    "circle",
    "sphere",
    "packing",
    "solver",
    "layout",
    "geometry",
    "apollonius",
    "apollonian"
  ],
  "author": "Akseli Pal√©n <akseli.palen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axelpale/apollonius/issues"
  },
  "homepage": "https://github.com/axelpale/apollonius#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.24.0",
    "standard": "^17.1.2",
    "tape": "^5.9.0",
    "typescript": "^5.6.3"
  }
}
